<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="16.0" layoutY="14.0" prefHeight="18.0" prefWidth="374.0" text="Aplikacja" textAlignment="CENTER" textFill="#396f2c">
         <font>
            <Font name="System Bold Italic" size="12.0" />
         </font>
      </Label>
      <StackedBarChart layoutX="15.0" layoutY="50.0" prefHeight="300.0" prefWidth="370.0">
        <xAxis>
          <CategoryAxis side="BOTTOM" />
        </xAxis>
        <yAxis>
          <NumberAxis side="LEFT" />
        </yAxis>
         <effect>
            <Glow />
         </effect>
      </StackedBarChart>
      <Button fx:id="applicationButton" layoutX="174.0" layoutY="350.0" mnemonicParsing="false" onAction="#appButtonAction" text="Return" />
   </children>
</AnchorPane>
